    "lecture-questions": {
      "tags": ["scope:app"]
    },
    "lecture-quizzes": {
      "tags": ["scope:app"]
    },
    "lecture-statistics": {
      "tags": ["scope:app"]
    },
    "lecture-watch": {
      "tags": ["scope:app"]
    },
    "login": {
      "tags": ["scope:app"]
    },
    "models": {
      "tags": ["scope:api"]
